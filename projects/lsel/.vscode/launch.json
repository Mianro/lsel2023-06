{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug linux",
            "type": "cppdbg",
            "request": "launch",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "linux": {
                "name": "Debug PC -linux",
                "type": "cppdbg",
                "request": "launch",
                "MIMode": "gdb",
                "miDebuggerPath": "gdb",
                "program": "${workspaceFolder}/output/${workspaceFolderBasename}"
            },
            "osx": {
                "name": "Debug PC -mac",
                "type": "cppdbg",
                "request": "launch",
                "MIMode": "lldb",
                "miDebuggerPath": "/Users/xxxxx/.vscode/extensions/ms-vscode.cpptools-1.13.8-darwin-x64/debugAdapters/lldb-mi/bin/lldb-mi",
                "program": "${workspaceFolder}/output/${workspaceFolderBasename}"
            },
            "windows": {
                "name": "Debug PC -win",
                "type": "cppdbg",
                "request": "launch",
                "MIMode": "gdb",
                "miDebuggerPath": "gdb.exe",
                "program": "${workspaceFolder}/output/${workspaceFolderBasename}.exe"
            }
        },
        {
            "name": "Ceedling - Debug test",
            "type": "cppdbg",
            "request": "launch",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "program": "${workspaceFolder}/ceedling/build/test/out/${command:ceedlingExplorer.debugTestExecutable}",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Debug device STM32F411RE",
            "type": "cortex-debug",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceFolder}/output/${workspaceFolderBasename}.elf",
            "request": "launch",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "device": "STM32F411RE",
            "configFiles": [
                "port/nucleo_stm32f411re/openocd.cfg"
            ],
            "preLaunchTask": "Clean & Re-Build NUCLEO_STM32F411RE",
            "svdFile": "../../svd/STM32F411.svd",
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 16000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    {
                        "type": "console",
                        "label": "ITM",
                        "port": 0
                    }
                ]
            }
        },
        {
            "name": "Debug device STM32F411E",
            "type": "cortex-debug",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceFolder}/output/${workspaceFolderBasename}.elf",
            "request": "launch",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "device": "STM32F411E",
            "configFiles": [
                "port/discovery_stm32f411E/openocd.cfg"
            ],
            "preLaunchTask": "BUILD DISCOVERY_STM32F411E",
            "svdFile": "../../svd/STM32F411.svd",
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 16000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    {
                        "type": "console",
                        "label": "ITM",
                        "port": 0
                    }
                ]
            }
        },
        {
            "name": "C/C++: gcc Compilar y depurar el archivo activo",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Habilitar la impresión con sangría para gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Establecer tipo de desensamblado en Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: gcc compilar archivo activo",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}